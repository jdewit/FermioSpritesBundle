{
    "name": "fermio/sprites-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for the simple image sprite and stylesheet generator.",
    "keywords": ["sprites"],
    "homepage": "http://sprites.readthedocs.org/",
    "license": "MIT",
    "authors": [{
        "name": "Pierre Minnieur",
        "email": "pierre@ferm.io",
        "homepage": "http://ferm.io/"
    }, {
        "name": "David Buchmann",
        "email": "mail@davidbu.ch"
    }],
    "require": {
        "php": "~5.3",
        "fermio/sprites": "dev-master",
        "symfony/framework-bundle": "~2.0"
    },
    "replace": {
        "pminnieur/sprites-bundle": "*"
    },
    "autoload": {
        "psr-0": {
            "Fermio\\Bundle\\SpritesBundle\\": ""
        }
    },
    "target-dir": "Fermio/Bundle/SpritesBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
